{
    "extends": "airbnb-base",
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "indent": 0,
        "linebreak-style": 0,
        "spaced-comment": 0,
        "no-undef": 0,
        "no-console": 0,
        "no-trailing-spaces": 0,
        "eol-last": 0,
        "no-unused-vars": 0,
        "consistent-return": "off"
    }
}